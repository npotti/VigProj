<?xml version="1.0" encoding="windows-1252" ?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:fraud="http://www.sbi.co.in/vigilance/smi"
            targetNamespace="http://www.sbi.co.in/vigilance/smi"
